{
  "name": "my-app",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "iosDebugBuild": "detox build -c ios.sim.debug",
    "iosReleaseBuild": "detox build -c ios.sim.release",
    "iosDebugE2E": "detox test -c ios.sim.debug",
    "iosDebugE2EHeadless": "detox test -c ios.sim.debug --headless",
    "iosRunModeDebug": "RUN_MODE=debug EXPO_USE_UPDATES=1 detox test -c ios.sim.debug",
    "iosReleaseE2E": "detox test -c ios.sim.release --cleanup --headless --record-logs all",
    "androidDebugBuild": "detox build -c android.emu.debug",
    "androidReleaseBuild": "detox build -c android.emu.release",
    "androidDebugE2E": "detox test -c android.emu.debug",
    "androidRunModeDebug": "RUN_MODE=debug EXPO_USE_UPDATES=1 detox test -c android.emu.debug",
    "androidReleaseE2E": "detox test -c android.emu.release --headless --record-logs all",
    "eas-build-pre-install": "./eas-hooks/eas-build-pre-install.sh",
    "eas-build-on-success": "./eas-hooks/eas-build-on-success.sh"
  },
  "dependencies": {
    "expo": "~48.0.18",
    "expo-dev-client": "~2.2.1",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.4",
    "expo-system-ui": "~2.2.1",
    "expo-updates": "~0.16.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.8",
    "react-native-web": "~0.18.11"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@config-plugins/detox": "^5.0.0",
    "@types/jest": "^29.4.0",
    "@types/react": "~18.0.27",
    "@types/react-native": "~0.70.6",
    "detox": "20.5.0",
    "jest": "^29.2.1",
    "jest-html-reporters": "^3.1.4",
    "sharp-cli": "^2.1.0",
    "typescript": "^4.9.4"
  },
  "private": true
}
