{
  "name": "my-app",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",

    "iosDebugBuild": "detox build -c ios.sim.debug",
    "iosReleaseBuild": "detox build -c ios.sim.release",
    
    "androidDebugBuild": "detox build -c android.emu.debug",
    "androidReleaseBuild": "detox build -c android.emu.release",
    
    "iosDebugE2E": "detox test -c ios.sim.debug",
    "androidDebugE2E": "detox test -c android.emu.debug",

    "iosReleaseE2E": "detox test -c ios.sim.release --cleanup",
    "androidReleaseE2E": "detox test -c android.emu.release",
    
    "androidAttDebugBuild": "detox build -c android.att.debug",
    "androidAttE2E": "detox test -c android.att.debug",
    
    "iosE2EReuse": "detox test -c ios.sim.debug --reuse",
    "androidE2EReuse": "detox test -c android.emu.debug --reuse"
  },
  "dependencies": {
    "expo": "~47.0.12",
    "expo-splash-screen": "~0.17.5",
    "expo-status-bar": "~1.4.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-web": "~0.18.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@config-plugins/detox": "^4.0.0",
    "@types/jest": "^29.4.0",
    "@types/react": "~18.0.14",
    "@types/react-native": "~0.70.6",
    "detox": "^20.1.5",
    "jest": "^29",
    "typescript": "^4.6.3"
  },
  "private": true
}
